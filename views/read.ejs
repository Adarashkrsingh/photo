<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
     <script src="https://cdn.tailwindcss.com"></script>
     
</head>
<body>
    <div class="text-black w-full h-full bg-zinc-900 p-10 " >
        <div class="text-blue-500  nav mb-10 ">
            <a  href="/">Go to Home page</a>
        </div>
        <h1  class=" text-blue-600 text-3xl tracking-tighter mb-3 ">READ</h1>
        
        <div class="users flex gap-10 flex-wrap">
           <% users.forEach(function(user){ %>
            <div class="user w-72 p-4 bg-zinc-800 rounded-lg">
                <div class="w-full h-52 rounded-lg bg-zinc-600 overflow-hidden">
                    <img class=" w-full h-full object-cover" src=
                    "<%=user.image%>" alt="">
                </div>
                <h3 class="text-xl mt-3 tracking-tighter text-yellow-500"><%=user.name%></h3>
                <h5 class="text-green-500">
                    <%= user.email%></h5>
                <div class="flex gap-3 mt-10">
                    <a class="text-green-800" href="/edit/<%= user._id%>">Edit user</a>
                    <a class="text-red-800" href="/delete/<%= user._id%>">Delete user</a>
                </div>
            </div>
           <% }) %>
            
            
            
           
            
            
        </div>
        
        </form>
    </div>
</body>
</html>